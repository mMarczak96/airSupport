FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      createPatchDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

pointSync false;

patches
(
    {
	name            rotor;
	patchInfo
	{
	type            patch;
	}
	constructFrom patches;
	patches (Rotor);
    }
    {
	name            rotor_zone_moving;
	patchInfo
	{
	type            cyclicAMI;
    	neighbourPatch  rotor_zone_moving;
   	transform       rotational;
	}
	constructFrom patches;
	patches (Rotor_zone_moving);
    } 
    {
	name            frontAndBack;
	patchInfo
	{
	type            empty;
	}
	constructFrom patches;
	patches (bottomEmptyFaces topEmptyFaces);
    }   
);

