FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      createPatchDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

pointSync false;

patches
(
    {
	name            inlet;
	patchInfo
	{
	type            patch;
	}
	constructFrom patches;
	patches (Inlet);
    }
    {
	name            outlet;
	patchInfo
	{
	type            patch;
	}
	constructFrom patches;
	patches (Outlet);
    }
    {
	name            sides;
	patchInfo
	{
	type            wall;
	}
	constructFrom patches;
	patches (Sides);
    }
    {
	name            rotor_zone_stationary ;
	patchInfo
	{
	type            cyclicAMI;
    	neighbourPatch  rotor_zone_moving;
   	transform       rotational;
	}
	constructFrom patches;
	patches (Rotor_zone_stationary);
    } 
    {
	name            frontAndBack;
	patchInfo
	{
	type            empty;
	}
	constructFrom patches;
	patches (bottomEmptyFaces topEmptyFaces);
    }   
);

